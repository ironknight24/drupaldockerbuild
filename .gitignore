# Ignore directories generated by Composer
/vendor/
/web/core/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore Drupal's user-uploaded files directory
/web/sites/*/files/
/web/sites/*/private/

# Ignore local settings, services, and development files.
/web/sites/*/settings.local.php
/web/sites/*/development.services.yml

# Ignore cache directories
/web/sites/*/php/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore IDE and editor-specific files
.idea/
.vscode/

# Ignore OS-specific files
.DS_Store

# Ignore compiled assets from custom themes
/web/themes/custom/*/css/
/web/themes/custom/*/js/

# Ignore Node.js dependencies
node_modules/

# Ignore local development and log files
*.log
